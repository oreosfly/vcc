{
    "extends": [

    ],
    "linterOptions": {
        "exclude": [
            "config/**/*.js",
            "node_modules/**/*.ts",
            "coverage/lcov-report/*.js",
            "*.json",
            "**/*.json",
            "server"
        ]
    },
    "rules": {
        "max-classes-per-file":false,
        "ordered-imports": [
            false
        ],
        "object-literal-sort-keys": [
            false
        ],
        "interface-name": false,
        "only-arrow-functions":false
    }
}